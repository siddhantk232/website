<pre class="src src-cpp"><span class="org-preprocessor">#include</span><span class="org-string">&lt;iostream&gt;</span>
<span class="org-preprocessor">#include</span><span class="org-string">&lt;iomanip&gt;</span>
<span class="org-keyword">using</span> <span class="org-keyword">namespace</span> <span class="org-constant">std</span>;

<span class="org-type">float</span> <span class="org-function-name">f</span>(<span class="org-type">float</span> <span class="org-variable-name">x</span>, <span class="org-type">float</span> <span class="org-variable-name">y</span>) {
  <span class="org-keyword">return</span> x + y * y;
}
<span class="org-type">int</span> <span class="org-function-name">main</span>() {
  <span class="org-type">float</span> <span class="org-variable-name">x0</span>, <span class="org-variable-name">y0</span>, <span class="org-variable-name">h</span>, <span class="org-variable-name">xn</span>, <span class="org-variable-name">x</span>, <span class="org-variable-name">y</span>, <span class="org-variable-name">k1</span>, <span class="org-variable-name">k2</span>, <span class="org-variable-name">k3</span>, <span class="org-variable-name">k4</span>, <span class="org-variable-name">k</span>;
  cout &lt;&lt; <span class="org-string">"Enter the values of x0, y0,"</span>
    &lt;&lt; <span class="org-string">"h, xn: "</span>;
  cin &gt;&gt; x0 &gt;&gt; y0 &gt;&gt; h &gt;&gt; xn;

  x = x0; y = y0;
  cout &lt;&lt; fixed;

  <span class="org-keyword">while</span>(1) {
    <span class="org-keyword">if</span> (x == xn) <span class="org-keyword">break</span>;
    k1 = h * f(x, y);
    k2 = h * f(x + h / 2, y + k1 / 2);
    k3 = h * f(x + h / 2, y + k2 / 2);
    k4 = h * f(x + h, y + k3);
    k = (k1 + (k2 + k3) * 2 + k4) / 6;
